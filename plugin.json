{
    "id": "com.natasquad.translate",
    "name": "Translator",
    "description": "This plugin is used to translate messages.",
    "homepage_url": "https://github.com/mattermost/mattermost-plugin-starter-template",
    "support_url": "https://github.com/mattermost/mattermost-plugin-starter-template/issues",
    "release_notes_url": "https://github.com/mattermost/mattermost-plugin-starter-template/releases/tag/v0.1.0",
    "icon_path": "assets/starter-template-icon.svg",
    "version": "0.1.0",
    "min_server_version": "6.2.1",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "settings_schema": {
        "settings": [
            {
                "key": "AWSAccessKeyID",
                "display_name": "AWS Access Key ID:",
                "type": "text",
                "help_text": "The access key ID from AWS."
            },
            {
                "key": "AWSSecretAccessKey",
                "display_name": "AWS Secret Access Key:",
                "type": "text",
                "help_text": "The secret access key from AWS."
            },
            {
                "key": "AWSRegion",
                "display_name": "AWS Region:",
                "type": "text",
                "help_text": "The region from AWS.",
                "default": "us-east-1"
            },
            {
                "key": "botID",
                "display_name": "BotId:",
                "type": "text",
                "help_text": "Id of translator bot."
            }
        ]
    }
}
